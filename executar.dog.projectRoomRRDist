echo "Calculando RR e Dist "
./repeatability ../dataset/projectRoom/H.BX.SX.txt ../dataset/projectRoom/BX/BX.dog1.txt ../dataset/projectRoom/SX/SX.dog1.txt 8 > ../dataset/projectRoom/RR/RR.dog1.txt

./repeatability ../dataset/projectRoom/H.BX.SX.txt ../dataset/projectRoom/BX/BX.dog.distribution.txt ../dataset/projectRoom/SX/SX.dog.distribution.txt 7 > ../dataset/projectRoom/Dist/distribution.dog.distribution.txt
