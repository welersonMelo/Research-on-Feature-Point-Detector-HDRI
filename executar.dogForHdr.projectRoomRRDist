echo "Calculando RR e Dist "
./repeatability ../dataset/projectRoom/H.BX.SX.txt ../dataset/projectRoom/BX/BX.dogForHDR1.txt ../dataset/projectRoom/SX/SX.dogForHDR1.txt 8 > ../dataset/projectRoom/RR/RR.dogForHDR1.txt

./repeatability ../dataset/projectRoom/H.BX.SX.txt ../dataset/projectRoom/BX/BX.dogForHDR.distribution.txt ../dataset/projectRoom/SX/SX.dogForHDR.distribution.txt 7 > ../dataset/projectRoom/Dist/distribution.dogForHDR.distribution.txt
